	prbmsum[i,] = prbrfull || prsqfull || probemsr || prbFfull || probedf1 || probedf2 || prbpfull;

	prbmdres[(ncol(bcpdes)*(i-1)+1):(ncol(bcpdes)*i),1] = probevec;
	prbmdres[(ncol(bcpdes)*(i-1)+1):(ncol(bcpdes)*i),2] = seprb;
