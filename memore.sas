		wvres[,wcount+3] = wvres[,wcount+1]/wvres[,wcount+2];
		wvres[,wcount+4] = 2*(1-probt(abs(wvres[,wcount+3]), df2));
		wvres[,(wcount+5):(wcount+6)] = wvres[,wcount+1]-tcritb*wvres[,wcount+2]|| wvres[,wcount+1]+tcritb*wvres[,wcount+2];
